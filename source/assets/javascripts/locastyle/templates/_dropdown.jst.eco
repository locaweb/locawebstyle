<div data-ls-module="dropdown" class="ls-dropdown">
  <a href="#" class="ls-btn"></a>
  <ul>
    <% for element in @elements: %>
      <% if element.text != '': %>
        <li><%- element %></li>
        <li><%= console.log(element) %></li>
        <li><a href="<%= element.href %>"><%= element.text %></a> </li>
      <% end %>
    <% end %>
  </ul>
</div>
